{
  "name": "danyg-go-to-test",
  "displayName": "Go To Test",
  "description": "Toggle between test and file with different strategies",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:danyg-go-to-test.goToTest"
  ],
  "main": "./out/src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "danyg-go-to-test.goToTest",
        "title": "Go To Test"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "clean": "rm -rf ./out",
    "compile": "npm run clean && tsc -p ./",
    "lint": "eslint ./ --ext ts",
    "watch": "tsc -watch -p ./",
    "test": "npm run test:all | gnomon",
    "test:all": "npm run test:pretest && npm run test:unit && npm run test:e2e",
    "test:pretest": "npm run compile && npm run lint",
    "test:e2e": "node ./out/test-helpers/run-e2e-tests.js",
    "test:unit": "node out/test-helpers/run-unit-tests.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.45.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "expect": "^26.0.1",
    "glob": "^7.1.6",
    "gnomon": "^1.5.0",
    "husky": "^4.2.5",
    "mocha": "^7.1.2",
    "prettier": "^2.0.5",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  }
}
